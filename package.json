{
	"name": "next-js-basic-authentication-email-credentials",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "dotenv -e .env.local npx prisma generate && next build",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest",
		"migrateInit": "dotenv -e .env.local -- npx prisma migrate dev --name init --skip-seed",
		"studio": "dotenv -e .env.local -- npx prisma studio",
		"postinstall": "dotenv -e .env.local npx prisma generate"
	},
	"prisma": {
		"seed": "dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "^5.12.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"next": "14.1.2",
		"react": "^18",
		"react-dom": "^18",
		"resend": "^3.2.0",
		"sqlite3": "^5.1.7",
		"tailwind-merge": "^2.2.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@testing-library/react": "^14.2.2",
		"@types/node": "^20.11.27",
		"@types/nodemailer": "^6.4.14",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.1.0",
		"jsdom": "^24.0.0",
		"postcss": "^8",
		"prisma": "^5.12.1",
		"tailwindcss": "^3.3.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.2",
		"vitest": "^1.4.0"
	}
}
